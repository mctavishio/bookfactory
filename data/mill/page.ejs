<script>
	window.addEventListener("load", e=> {
		let cardsEl = document.querySelector("#cards");
		p.drawings.forEach( (drawing,j) => {
			let divEl = document.createElement("div");
			divEl.setAttribute("class", "cardflexitem");
			let figureEl = document.createElement("figure");
			let imageEl = document.createElement("img");
			let src = "algorithm-<%= alogorithm %>_printrun-<%= printrun %>_size-<%= size %>_" + (j+1).toString().padStart(2, "0")+"-small.png";
			console.log(src);
			imageEl.src = src;
			figureEl.appendChild(imageEl);
			divEl.appendChild(figureEl);
			cardsEl.appendChild(divEl);
			let largesrc = "algorithm-<%= alogorithm %>_printrun-<%= printrun %>_size-<%= size %>_" + (j+1).toString().padStart(2, "0")+".png";
			imageEl.addEventListener("click", e => {window.open(largesrc)});
		});
		let timestampEl = document.querySelector("#timestamp");
		timestamp.innerText = "@ => "+p.datetime;
		let dataEl = document.querySelector("#rawdata");
		dataEl.innerText = JSON.stringify(p,null,2);
		let pdfframesEl = document.querySelector("#pdfframes");
		p.drawings.forEach( (drawing,j) => {
			let liEl = document.createElement("li");
			let aEl = document.createElement("a");
			let url = 
			aEl.setAttribute("href", "algorithm-<%= alogorithm %>_printrun-<%= printrun %>_size-<%= size %>_" + (j+1).toString().padStart(2, "0")+".pdf");
			aEl.innerText = "page: "+(j+1).toString().padStart(2, "0");
			liEl.appendChild(aEl);
			pdfframesEl.appendChild(liEl);
		});
	});
</script>
